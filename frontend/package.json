{
  "name": "frontend",
  "packageManager": "yarn@3.6.1",
  "scripts": {
    "type-check": "tsc --noEmit",
    "type-check:watch": "yarn type-check --watch",
    "start:dev": "cross-env NODE_ENV=development webpack serve --config config/webpack.dev.js",
    "start:prod": "serve -s dist",
    "build:prod": "cross-env NODE_ENV=production webpack --progress --config config/webpack.prod.js",
    "build:sb": "sb build",
    "sb": "storybook dev -p 6006",
    "prepare": "cd .. && husky install frontend/.husky && chmod +x ./.husky/*"
  },
  "devDependencies": {
    "@babel/core": "^7.22.9",
    "@babel/preset-env": "^7.22.9",
    "@babel/preset-react": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.10",
    "@storybook/addon-essentials": "^7.0.24",
    "@storybook/addon-interactions": "^7.0.26",
    "@storybook/addon-links": "^7.0.24",
    "@storybook/blocks": "^7.0.24",
    "@storybook/global": "^5.0.0",
    "@storybook/jest": "^0.1.0",
    "@storybook/preset-create-react-app": "^7.0.24",
    "@storybook/react": "^7.0.24",
    "@storybook/react-webpack5": "^7.0.24",
    "@storybook/testing-library": "^0.2.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@types/babel__core": "^7",
    "@types/babel__preset-env": "^7",
    "@types/node": "^20.4.2",
    "@types/prettier": "^2",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/styled-components": "^5.1.26",
    "@types/testing-library__jest-dom": "^5.14.8",
    "@typescript-eslint/eslint-plugin": "latest",
    "@typescript-eslint/parser": "latest",
    "babel-loader": "^9.1.3",
    "babel-plugin-styled-components": "^2.1.4",
    "babel-preset-react-app": "^10.0.1",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "dotenv-expand": "^10.0.0",
    "eslint": "^8.44.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "latest",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "fork-ts-checker-webpack-plugin": "^8.0.0",
    "html-webpack-plugin": "^5.5.3",
    "husky": ">=7",
    "lint-staged": ">=10",
    "msw": "^1.2.2",
    "pinst": ">=2",
    "postcss": "^8.4.26",
    "postcss-styled-syntax": "^0.4.0",
    "prettier": "2.8.8",
    "react-refresh": "^0.14.0",
    "serve": "^14.2.0",
    "storybook": "^7.0.24",
    "styled-components": "^6.0.4",
    "stylelint": "^15.10.1",
    "stylelint-config-clean-order": "^5.0.1",
    "stylelint-config-standard": "^34.0.0",
    "terser-webpack-plugin": "^5.3.9",
